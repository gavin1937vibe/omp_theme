{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#DD4814",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}"
        },
        {
          "foreground": "red",
          "style": "plain",
          "template": " % ",
          "type": "root"
        },
        {
          "style": "plain",
          "template": "<cyan> {{ .UserName }}</> ",
          "type": "session"
        },
        {
          "foreground": "lightYellow",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "<darkGray>in </>{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "white",
          "properties": {
            "fetch_version": true,
            "fetch_virtual_env": true
          },
          "style": "plain",
          "template": "({{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }})",
          "type": "python"
        },
        {
          "foreground": "red",
          "style": "plain",
          "template": " C:{{ if gt .Code 0 }}{{ .Code }}{{ end }} ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#ffffff",
          "template": " <#fefefe>î®¢</> {{ .FormattedMs }} ",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": true
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "white",
          "style": "plain",
          "template": "[{{ .CurrentDate | date .Format }}]",
          "type": "time"
        },
        {
          "foreground": "green",
          "style": "plain",
          "template": " $",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
