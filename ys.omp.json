{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#DD4814",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
        },
        {
          "type": "root",
          "foreground": "red",
          "style": "plain",
          "template": " % "
        },
        {
          "type": "session",
          "style": "plain",
          "template": "<cyan> {{ .UserName }}</> "
        },
        {
          "type": "path",
          "foreground": "lightYellow",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "<darkGray>@ </>{{ .Path }} "
        },
        {
          "type": "git",
          "style": "plain",
          "template": "({{ .HEAD }})",
          "foreground": "#FFA500",
          "properties": {
            "fetch_status": false,
            "fetch_upstream_icon": false
          }
        },
        {
          "type": "status",
          "foreground": "#FF0F0F",
          "style": "plain",
          "template": " C:{{ if gt .Code 0 }}{{ .Code }}{{ end }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "time",
          "foreground": "white",
          "style": "plain",
          "template": "[{{ .CurrentDate | date .Format }}]"
        },
        {
          "type": "python",
          "foreground": "#0277BD",
          "properties": {
            "fetch_version": true,
            "fetch_virtual_env": true
          },
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}îˆµ {{ .Venv }}{{ end }}{{ end }}"
        },
        {
          "type": "text",
          "foreground": "green",
          "style": "plain",
          "template": " $"
        }
      ]
    }
  ],
  "final_space": true,
  "version": 3
}
